<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.ds.login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>aas1</servlet-name>
		<servlet-class>com.ds.apppends</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>aas1</servlet-name>
		<url-pattern>/apppends</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>session</servlet-name>
		<servlet-class>com.ds.session</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>session</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.ds.logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Blogger</servlet-name>
		<servlet-class>com.ds.BloggerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Blogger</servlet-name>
		<url-pattern>/blogger</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>r</servlet-name>
		<servlet-class>com.ds.register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>r</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>session</welcome-file>
	</welcome-file-list>
</web-app>
